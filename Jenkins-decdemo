pipeline{
    agent any
    stages{
        stage("GIT checkout"){
            steps{
                git credentialsId: 'myhome', url: 'https://github.com/indumathiu4/myweb.git'
                
            }
        }
        stage("Maven Build"){
            steps{
             bat /"%MVN_HOME%\bin\mvn" -Dmaven.test.failure.ignore clean package/
               
            }
         }
    }
}
